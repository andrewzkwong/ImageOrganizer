<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="topPaneAddDialog" prefHeight="134.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.imageorganizer.AddDialogController">
  <children>
    <Button fx:id="setFolderButton" layoutX="163.0" layoutY="57.0" mnemonicParsing="false" onAction="#setFolderAction" text="Set Folder" />
    <Button fx:id="setKeyButton" layoutX="411.0" layoutY="57.0" mnemonicParsing="false" onAction="#setKeyAction" text="Set Key" />
    <TextField fx:id="folderNameField" layoutX="31.0" layoutY="24.0" prefWidth="337.0" />
    <TextField fx:id="keyNameField" layoutX="391.0" layoutY="24.0" prefWidth="99.0" />
    <Button fx:id="okAddDialog" layoutX="464.0" layoutY="92.0" mnemonicParsing="false" onAction="#finishAddAction" text="OK" />
    <Button fx:id="cancelAddDialog" layoutX="530.0" layoutY="92.0" mnemonicParsing="false" text="Cancel" onAction="#cancelAddAction" />
  </children>
</AnchorPane>
